<% include header %>
<body class="body-bg1">
    <div class="main">
        <div class="main-content">
            <div class="form">
                <h1>
                    <q>
                        <%= title %>
                    </q>
                </h1>
                <form method="post" action="repair" enctype="multipart/form-data">
                    <ul>
                        <li>
                            <i class="iconfont color-cheng">&#xe60e;</i>
                            <input type="text" name="name" placeholder="请输入姓名">
                        </li>
                        <li>
                            <i class="iconfont color-zi">&#xe614;</i>
                            <input type="text" name="contact" placeholder="请输入联系方式">
                        </li>
                        <li>
                            <i class="iconfont color-lan">&#xe619;</i>
                            <input type="text" name="dorm" placeholder="请输入需要上门维修的宿舍号">
                        </li>
                        <li>
                            <i class="iconfont color-huang">&#xe602;</i>
                            <input type="text" name="info" placeholder="故障描述">
                        </li>
                    </ul>
                    <input type="submit" value="继续">
                </form>
            </div>
        </div>
    </div>
<% include footer %>